{
    "name": "OpenSwoole with Databases",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "openswoole",
    "workspaceFolder": "/workspace",
    "forwardPorts": [9501],
    "extensions": [
      "felixfbecker.php-debug",
      "ms-vscode.php"
    ],
    "settings": {
      "php.executablePath": "/usr/local/bin/php"
    },
    "postCreateCommand": "docker-compose up -d"
  }